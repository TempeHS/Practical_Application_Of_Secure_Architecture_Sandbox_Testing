

# Database files (excluding *.db files which are now tracked with regular Git)
*.sqlite filter=lfs diff=lfs merge=lfs -text
*.sqlite3 filter=lfs diff=lfs merge=lfs -text

# Binary executables
*.exe filter=lfs diff=lfs merge=lfs -text
*.bin filter=lfs diff=lfs merge=lfs -text
*.dmg filter=lfs diff=lfs merge=lfs -text
*.iso filter=lfs diff=lfs merge=lfs -text

# Docker images and container files
*.tar filter=lfs diff=lfs merge=lfs -text

# Large log files (over 10MB)
*.log filter=lfs diff=lfs merge=lfs -text

# Compiled Python files and caches (usually generated, but good to track if committed)
*.pyc filter=lfs diff=lfs merge=lfs -text
__pycache__/ filter=lfs diff=lfs merge=lfs -text
